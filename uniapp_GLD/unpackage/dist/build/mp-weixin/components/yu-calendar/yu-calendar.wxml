<view class="yu-calendar data-v-0bd508d6" style="background-color:#6fb950;margin-top:30%;"><view class="yu-calendar-header data-v-0bd508d6"><view data-event-opts="{{[['tap',[['prevMonth']]]]}}" class="header-left data-v-0bd508d6" bindtap="__e">上一月</view><view class="header-center data-v-0bd508d6">{{year+'年'+selectMonth+"月"}}</view><view data-event-opts="{{[['tap',[['nextMonth']]]]}}" class="header-right data-v-0bd508d6" bindtap="__e">下一月</view></view><view class="yu-calendar-week data-v-0bd508d6"><block wx:for="{{weeks}}" wx:for-item="week" wx:for-index="idx" wx:key="idx"><view class="list data-v-0bd508d6">{{week}}</view></block></view><view class="yu-calendar-content data-v-0bd508d6"><view class="yu-calendar-day data-v-0bd508d6"><block wx:for="{{dayArr}}" wx:for-item="day" wx:for-index="idx" wx:key="idx"><view data-event-opts="{{[['tap',[['onClick',['$0',idx],[[['dayArr','',idx]]]]]]]}}" class="list data-v-0bd508d6" bindtap="__e"><block wx:if="{{day.date!=localDate}}"><view class="day data-v-0bd508d6" style="{{'background:'+(day.bgColor)+';'+('color:'+(day.color)+';')}}">{{day.day}}</view></block><block wx:else><view class="day data-v-0bd508d6" style="{{'background:'+(localColor)+';'+('color:'+(color)+';')}}">{{day.day}}</view></block></view></block><view class="local-month data-v-0bd508d6">{{selectMonth}}</view></view></view></view>