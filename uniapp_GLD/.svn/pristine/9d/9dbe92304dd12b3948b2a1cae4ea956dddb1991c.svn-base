<view class="xweek_box"><view class="xweek_ym"><text data-event-opts="{{[['tap',[['prevYear',['$event']]]]]}}" bindtap="__e"><text class="i prev_i"></text></text><text class="ym_text">{{y+"年"}}</text><text data-event-opts="{{[['tap',[['nextYear',['$event']]]]]}}" bindtap="__e"><text class="i next_i"></text></text><text data-event-opts="{{[['tap',[['prevMonth',['$event']]]]]}}" bindtap="__e"><text class="i prev_i"></text></text><text class="ym_text">{{m+1+"月"}}</text><text data-event-opts="{{[['tap',[['nextMonth',['$event']]]]]}}" bindtap="__e"><text class="i next_i"></text></text><text data-event-opts="{{[['tap',[['prevWeek',['$event']]]]]}}" bindtap="__e"><text class="i prev_i"></text></text><text class="ym_text">周</text><text data-event-opts="{{[['tap',[['nextWeek']]]]}}" bindtap="__e"><text class="i next_i"></text></text></view><view class="xweek_title"><text>一</text><text>二</text><text>三</text><text>四</text><text>五</text><text>六</text><text>日</text></view><view class="xweek_day"><view class="{{['day_body',dayClass]}}"><block wx:for="{{days}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="day"><text data-event-opts="{{[['tap',[['change',['$0'],[[['days','',index]]]]]]]}}" class="{{[item.sel?'sel':'']}}" bindtap="__e">{{item.d}}</text></view></block></view></view></view>